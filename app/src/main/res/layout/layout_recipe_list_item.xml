<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/theme_padding_ten"
    android:paddingBottom="@dimen/theme_padding_ten"
    android:paddingLeft="@dimen/theme_padding_ten"
    android:paddingRight="@dimen/theme_padding_ten"
    android:orientation="vertical">

    <TextView
        android:id="@+id/txt_two_level"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/theme_color_black_a"
        android:textSize="@dimen/theme_text_size_fourteen"/>

    <lib.widget.FlowLayout
        android:id="@+id/flow_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/theme_margin_ten">

    </lib.widget.FlowLayout>
</LinearLayout>